<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/p_c">

    <Button
        android:id="@+id/contact1But"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        android:text="contact 1" />

    <Button
        android:id="@+id/contact2But"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/contact1But"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:text="contact 2" />

    <Button
        android:id="@+id/contact3But"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@+id/contact2But"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:text="contact 3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/w_c"
        android:layout_below="@id/contact3But"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       android:layout_below="@id/textView4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:textColor="@color/w_c"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:textColor="@color/w_c"/>
</RelativeLayout>
